package com.spacecombat;

public class Text extends Component {
	
	private String text;
	private DrawableText drawableText;
	
	public Text (String text, DrawableText drawText)
	{
		this.text = text;
		this.drawableText = drawText;
	}
	
	public void draw ()
	{
		drawableText.draw();
	}

}
